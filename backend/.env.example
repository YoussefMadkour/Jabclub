# Database
DATABASE_URL="postgresql://username:password@localhost:5432/jabclub?schema=public"

# JWT
JWT_SECRET="your-secret-key-change-in-production"
JWT_EXPIRES_IN="24h"

# Server
PORT=5000
NODE_ENV="development"

# File Upload
UPLOAD_DIR="./uploads"
MAX_FILE_SIZE=5242880

# CORS
FRONTEND_URL="http://localhost:3000"

# Session secret for express-session
SESSION_SECRET=your-secret-key-change-this-in-production

# QR Code signature secret for security
QR_SECRET=change-this-to-a-random-64-character-string-in-production

# Upstash Redis (optional but recommended for performance)
# Get these from https://console.upstash.com â€” create a free Redis database
UPSTASH_REDIS_REST_URL=https://your-redis-url.upstash.io
UPSTASH_REDIS_REST_TOKEN=your-upstash-redis-token

# Google OAuth Configuration
GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your-google-client-secret
GOOGLE_CALLBACK_URL=http://localhost:5000/api/auth/google/callback
